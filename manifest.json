{
  "manifest_version": 2,
  "name": "SHOUTcast Player (Queercraft Radio)",
  "version": "2.0",
  "description": "Plays Audio From Queercraft Radio",
  "homepage_url": "https://github.com/BusterBean/QC-SHOUTcast-Player-FireFox-Extension",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_popup": "popup/popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "64": "icons/icon64.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "64": "icons/icon64.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "storage",
    "activeTab",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "applications": {
    "gecko": {
      "id": "da0b6fb824f34431bb3d495ed7ef974c@queercraft.net"
    }
  }
}
